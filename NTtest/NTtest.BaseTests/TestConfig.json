{
  "Timeouts": {
    "Quick": 100,
    "Short": 1000,
    "Medium": 3000,
    "Long": 10000,
    "Implicit": 20000
  },
  "Browser": {
    "Type": "Chrome",
    "Version": "79.0",
    "IsRemote": false
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "${basedir}\\nlog-${shortdate}.log"
      },
      "logconsole": {
        "type": "Console",
        "layout": "${longdate}|${level}|${callsite}|${message}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logfile"
      }
    ]
  }
}